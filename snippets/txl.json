{
    "include": {
        "prefix": "include",
        "scope": "txl",
        "body": [
            "include \"${1:fileName}\""
        ]
    },
    "define": {
        "prefix": "define",
        "scope": "txl",
        "body": [
            "define ${1:name}",
            "\t$0",
            "end define"
        ]
    },
    "redefine": {
        "prefix": "redefine",
        "scope": "txl",
